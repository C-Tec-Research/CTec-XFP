<Page x:Class="Xfp.UI.Views.PanelTools.CausesAndEffects"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Xfp.UI.Views.PanelTools"
      xmlns:ctc="clr-namespace:CTecControls.UI;assembly=CTecControls"
      xmlns:ctcvh="clr-namespace:CTecControls.UI.ViewHelpers;assembly=CTecControls"
      xmlns:vh="clr-namespace:Xfp.UI.ViewHelpers"
      xmlns:viewmodels="clr-namespace:Xfp.ViewModels.PanelTools" 
      xmlns:val="clr-namespace:Xfp.UI.Validation"
      d:DataContext="{d:DesignInstance Type=viewmodels:CausesAndEffectsViewModel}"
      mc:Ignorable="d" 
      Title="CausesAndEffects">

    <Page.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <ctcvh:BlankToNotAvailableConverter x:Key="BlankToNotAvailableConverter"/>
        <ctcvh:BlankToItalicConverter x:Key="BlankToItalicConverter"/>
        <ctcvh:BoolToBoldConverter x:Key="BoolToBoldConverter"/>
        <ctcvh:InverseBoolToBoldConverter x:Key="InverseBoolToBoldConverter"/>
        <ctcvh:BoolToInverseConverter x:Key="BoolToInverseConverter"/>
        <ctcvh:BoolToOpacity40pcConverter x:Key="BoolToOpacity40pcConverter"/>
        <ctcvh:BoolToErrorColourConverter x:Key="BoolToErrorColourConverter"/>
        <ctcvh:InverseBoolToErrorColourConverter x:Key="InverseBoolToErrorColourConverter"/>
        <ctcvh:InverseBoolToOpacity40pcConverter x:Key="InverseBoolToOpacity40pcConverter"/>
    </Page.Resources>


    <Grid Margin="10,0,0,0">
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <Grid Margin="0,5,10,5">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                
                <TextBlock Grid.Row="0" Text="{Binding PageHeader}" Style="{StaticResource PageTitle}"/>
                
                <!--#region timer event times-->
                <WrapPanel Grid.Row="1" Margin="0,0,0,10">
                    <Border Style="{StaticResource InputPanelBorder}" Padding="8,3,8,5">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="2"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="17" Orientation="Vertical">
                                <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="17" Text="{Binding Path=Timer_Event_Times, Source={StaticResource Resources}}" FontWeight="DemiBold" HorizontalAlignment="Left" Margin="0" Padding="0"/>
                                <Border Grid.Row="1" Grid.Column="2" BorderBrush="{StaticResource Brush06}" BorderThickness="0,1,0,0" Margin="0,0,0,5"/>
                            </StackPanel>

                            <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding Path=Timer_Event, Source={StaticResource Resources}}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,3,-1"/>
                            <TextBlock Grid.Row="2" Grid.Column="0" Text="{Binding Path=Occurs_At, Source={StaticResource Resources}}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,3,3"/>
                            <TextBlock Grid.Row="4" Grid.Column="0" Text="{Binding Path=Timer_Event, Source={StaticResource Resources}}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,2,3,-1"/>
                            <TextBlock Grid.Row="5" Grid.Column="0" Text="{Binding Path=Occurs_At, Source={StaticResource Resources}}" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,3,3"/>

                            <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding TimerHeaders[0]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="2" Text="{Binding TimerHeaders[1]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="3" Text="{Binding TimerHeaders[2]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="4" Text="{Binding TimerHeaders[3]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="5" Text="{Binding TimerHeaders[4]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="6" Text="{Binding TimerHeaders[5]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="7" Text="{Binding TimerHeaders[6]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="1" Grid.Column="8" Text="{Binding TimerHeaders[7]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding TimerHeaders[8]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="2" Text="{Binding TimerHeaders[9]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="3" Text="{Binding TimerHeaders[10]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="4" Text="{Binding TimerHeaders[11]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="5" Text="{Binding TimerHeaders[12]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="6" Text="{Binding TimerHeaders[13]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="7" Text="{Binding TimerHeaders[14]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                            <TextBlock Grid.Row="4" Grid.Column="8" Text="{Binding TimerHeaders[15]}" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>

                            <ctc:TimePicker Grid.Row="2" Grid.Column="1" x:Name="tp1" Value="{Binding TimerEvent1}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="0,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="2" x:Name="tp2" Value="{Binding TimerEvent2}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="3" x:Name="tp3" Value="{Binding TimerEvent3}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="4" x:Name="tp4" Value="{Binding TimerEvent4}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="5" x:Name="tp5" Value="{Binding TimerEvent5}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="6" x:Name="tp6" Value="{Binding TimerEvent6}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="7" x:Name="tp7" Value="{Binding TimerEvent7}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="2" Grid.Column="8" x:Name="tp8" Value="{Binding TimerEvent8}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="1" x:Name="tp9" Value="{Binding TimerEvent9}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="2" x:Name="tp10" Value="{Binding TimerEvent10}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="3" x:Name="tp11" Value="{Binding TimerEvent11}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="4" x:Name="tp12" Value="{Binding TimerEvent12}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="5" x:Name="tp13" Value="{Binding TimerEvent13}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="6" x:Name="tp14" Value="{Binding TimerEvent14}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="7" x:Name="tp15" Value="{Binding TimerEvent15}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                            <ctc:TimePicker Grid.Row="5" Grid.Column="8" x:Name="tp16" Value="{Binding TimerEvent16}" Format="HH:mm" PreviewMouseDown="ctrl_PreviewMouseDown" PreviewMouseWheel="ctrl_PreviewMouseWheel" Margin="3,-2,0,0"/>
                        </Grid>
                    </Border>
                </WrapPanel>
                <!--#endregion-->

                <!--#region triggers and actions-->
                <Border Grid.Row="2" Grid.ColumnSpan="2" Style="{StaticResource InputPanelBorder}" Padding="0,0,2,0">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                            <RowDefinition Height="Auto" MinHeight="24"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>                <!--0 - num-->
                            <ColumnDefinition Width="5"/>                   <!--  (grid line) -->
                            <ColumnDefinition Width="Auto" MinWidth="100"/> <!--2 - action type-->
                            <ColumnDefinition Width="Auto" MinWidth="100"/> <!--3 - action param-->
                            <ColumnDefinition Width="5"/>                   <!--  (grid line) -->
                            <ColumnDefinition Width="Auto"/>                <!--5 - trigger type-->
                            <ColumnDefinition Width="Auto"/>                <!--6 - trigger param-->
                            <ColumnDefinition Width="Auto" MinWidth="40"/>  <!--7 - trigger condition-->
                            <ColumnDefinition Width="6"/>                   <!--  (grid line)-->
                            <ColumnDefinition Width="Auto" MinWidth="100"/> <!--9 - reset type-->
                            <ColumnDefinition Width="Auto" MinWidth="100"/> <!--10 - reset param-->
                            <ColumnDefinition Width="Auto"/>                <!--11 - reset condition-->
                        </Grid.ColumnDefinitions>

                        <!--#region headers-->
                        <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="1" Text="{Binding Path=Number_Symbol, Source={StaticResource Resources}}" Background="{StaticResource Brush09}" FontWeight="Bold" HorizontalAlignment="Right" Margin="0" Padding="0,0,0,0"/>
                        <TextBlock Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2" Text="{Binding Path=Action, Source={StaticResource Resources}}" Background="{StaticResource Brush09}" FontWeight="Bold" Margin="1,0,0,0" Padding="0,0,5,0"/>
                        <TextBlock Grid.Row="0" Grid.Column="5" Grid.ColumnSpan="3" Text="{Binding Path=Occurs_When, Source={StaticResource Resources}}" Background="{StaticResource Brush09}" FontWeight="Bold" Margin="2,0,0,0" Padding="0,0,5,0"/>
                        <TextBlock Grid.Row="0" Grid.Column="9" Grid.ColumnSpan="3" Text="{Binding Path=Resets_When, Source={StaticResource Resources}}" Background="{StaticResource Brush09}" FontWeight="Bold" Margin="1,0,0,0" Padding="0,0,5,0"/>
                        <!--#endregion-->

                        <!--#region gridlines-->
                        <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,1,0,0" BorderBrush="{StaticResource Brush06}" Margin="0,22,0,0"/>
                        <Border Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="7" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="8" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="9" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="10" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="11" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="12" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="13" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="14" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="15" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="16" Grid.Column="0" Grid.ColumnSpan="18" BorderThickness="0,0.5,0,0" BorderBrush="{StaticResource Brush07}"/>
                        <Border Grid.Row="0" Grid.Column="1" Grid.RowSpan="17" BorderThickness="0.5,0,0,0" BorderBrush="{StaticResource Brush07}" Margin="2,0,0,0"/>
                        <Border Grid.Row="0" Grid.Column="4" Grid.RowSpan="17" BorderThickness="0.5,0,0,0" BorderBrush="{StaticResource Brush07}" Margin="1.5,0,0,0"/>
                        <Border Grid.Row="0" Grid.Column="8" Grid.RowSpan="17" BorderThickness="0.5,0,0,0" BorderBrush="{StaticResource Brush07}" Margin="3,0,0,0"/>
                        <!--#endregion-->

                        
                        <!--#region C & E number-->
                        <TextBlock Grid.Row="1"  Grid.Column="0" Text="{Binding CEConfigItems[0].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="2"  Grid.Column="0" Text="{Binding CEConfigItems[1].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="3"  Grid.Column="0" Text="{Binding CEConfigItems[2].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="4"  Grid.Column="0" Text="{Binding CEConfigItems[3].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="5"  Grid.Column="0" Text="{Binding CEConfigItems[4].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="6"  Grid.Column="0" Text="{Binding CEConfigItems[5].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="7"  Grid.Column="0" Text="{Binding CEConfigItems[6].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="8"  Grid.Column="0" Text="{Binding CEConfigItems[7].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="9"  Grid.Column="0" Text="{Binding CEConfigItems[8].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="10" Grid.Column="0" Text="{Binding CEConfigItems[9].CENumber}"  HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="11" Grid.Column="0" Text="{Binding CEConfigItems[10].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="12" Grid.Column="0" Text="{Binding CEConfigItems[11].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="13" Grid.Column="0" Text="{Binding CEConfigItems[12].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="14" Grid.Column="0" Text="{Binding CEConfigItems[13].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="15" Grid.Column="0" Text="{Binding CEConfigItems[14].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <TextBlock Grid.Row="16" Grid.Column="0" Text="{Binding CEConfigItems[15].CENumber}" HorizontalAlignment="Right" Margin="3,0,0,0"/>
                        <!--#endregion-->

                        
                        <!--#region action type-->
                        <ComboBox Grid.Row="1"   Grid.Column="2" ItemsSource="{Binding CEItem1.Actions}" SelectedItem="{Binding CEItem1.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="2"   Grid.Column="2" ItemsSource="{Binding CEItem2.Actions}" SelectedItem="{Binding CEItem2.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="3"   Grid.Column="2" ItemsSource="{Binding CEItem3.Actions}" SelectedItem="{Binding CEItem3.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="4"   Grid.Column="2" ItemsSource="{Binding CEItem4.Actions}" SelectedItem="{Binding CEItem4.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="5"   Grid.Column="2" ItemsSource="{Binding CEItem5.Actions}" SelectedItem="{Binding CEItem5.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="6"   Grid.Column="2" ItemsSource="{Binding CEItem6.Actions}" SelectedItem="{Binding CEItem6.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="7"   Grid.Column="2" ItemsSource="{Binding CEItem7.Actions}" SelectedItem="{Binding CEItem7.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="8"   Grid.Column="2" ItemsSource="{Binding CEItem8.Actions}" SelectedItem="{Binding CEItem8.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="9"   Grid.Column="2" ItemsSource="{Binding CEItem9.Actions}" SelectedItem="{Binding CEItem9.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="10"  Grid.Column="2" ItemsSource="{Binding CEItem10.Actions}" SelectedItem="{Binding CEItem10.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="11"  Grid.Column="2" ItemsSource="{Binding CEItem11.Actions}" SelectedItem="{Binding CEItem11.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="12"  Grid.Column="2" ItemsSource="{Binding CEItem12.Actions}" SelectedItem="{Binding CEItem12.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="13"  Grid.Column="2" ItemsSource="{Binding CEItem13.Actions}" SelectedItem="{Binding CEItem13.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="14"  Grid.Column="2" ItemsSource="{Binding CEItem14.Actions}" SelectedItem="{Binding CEItem14.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="15"  Grid.Column="2" ItemsSource="{Binding CEItem15.Actions}" SelectedItem="{Binding CEItem15.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <ComboBox Grid.Row="16"  Grid.Column="2" ItemsSource="{Binding CEItem16.Actions}" SelectedItem="{Binding CEItem16.SelectedActionType}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                        <!--#endregion-->


                        <!--#region action param-->
                        <Border Grid.Row="1" Grid.Column="3" Visibility="{Binding CEItem1.ShowActionParam, Converter={StaticResource BoolToVisibilityConverter}}"
                                BorderBrush="{Binding CEItem1.ActionParamIsValid, Converter={StaticResource InverseBoolToErrorColourConverter}}" BorderThickness="1" Padding="1,0,1,0">
                            <Grid>
                                <ComboBox ItemsSource="{Binding CEItem1.Loop1Devices}" SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionLoop1Devices,  Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"    MaxDropDownHeight="500"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Loop2Devices}" SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionLoop2Devices,  Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"    MaxDropDownHeight="500"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Relays}"       SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionRelays,        Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Groups}"       SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionGroups,        Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Zones}"        SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionZones,         Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Sets}"         SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionSets,          Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                                <ComboBox ItemsSource="{Binding CEItem1.SetsRelays}"   SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionSetsAndRelays, Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                                <ComboBox ItemsSource="{Binding CEItem1.Events}"       SelectedIndex="{Binding CEItem1.SelectedActionParamIndex}" Visibility="{Binding CEItem1.ShowActionEvents,        Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"/>
                            </Grid>
                        </Border>
                        <Border Grid.Row="2" Grid.Column="3" Visibility="{Binding CEItem2.ShowActionParam, Converter={StaticResource BoolToVisibilityConverter}}"
                                BorderBrush="{Binding CEItem2.ActionParamIsValid, Converter={StaticResource InverseBoolToErrorColourConverter}}" BorderThickness="1" Padding="1,0,1,0">
                            <Grid>
                                <ComboBox ItemsSource="{Binding CEItem2.Loop1Devices}" SelectedIndex="{Binding CEItem2.SelectedActionParamIndex}" Visibility="{Binding CEItem2.ShowActionLoop1Devices,  Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"    MaxDropDownHeight="500"/>
                                <ComboBox ItemsSource="{Binding CEItem2.Loop2Devices}" SelectedIndex="{Binding CEItem2.SelectedActionParamIndex}" Visibility="{Binding CEItem2.ShowActionLoop2Devices,  Converter={StaticResource BoolToVisibilityConverter}}" PreviewMouseDown="combo_PreviewMouseDown" MinWidth="60"    MaxDropDownHeight="500"/>
