<Page x:Class="Xfp.UI.Views.PanelTools.GroupConfig"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Xfp.UI.Views.PanelTools"
      xmlns:ctc="clr-namespace:CTecControls.UI;assembly=CTecControls" 
      xmlns:ctcb="clr-namespace:CTecControls.UI.Behaviours;assembly=CTecControls" 
      xmlns:ctce="clr-namespace:CTecControls.UI.Extensions;assembly=CTecControls"
      xmlns:ctcvh="clr-namespace:CTecControls.UI.ViewHelpers;assembly=CTecControls"
      xmlns:viewmodels="clr-namespace:Xfp.ViewModels.PanelTools" 
      xmlns:vh="clr-namespace:Xfp.UI.ViewHelpers"
      xmlns:val="clr-namespace:Xfp.UI.Validation"
      d:DataContext="{d:DesignInstance Type=viewmodels:GroupConfigViewModel}"
      mc:Ignorable="d" 
      Title="GroupConfig">

    <Page.Resources>
        <!--<BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter"/>-->
        <ctcvh:BoolToInverseConverter x:Key="BoolToInverseConverter"/>
        <ctcvh:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <ctcvh:InverseBoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter"/>
        <ctcvh:BoolToOpacity40pcConverter x:Key="BoolToOpacity40pcConverter"/>
        <ctcvh:InverseBoolToOpacity40pcConverter x:Key="InverseBoolToOpacity40pcConverter"/>
        <ctcvh:IsNullOrWhiteSpaceToBoolConverter x:Key="IsNullOrWhiteSpaceToBoolConverter"/>
        <ctcvh:IsNullOrWhiteSpaceToOpacityConverter x:Key="IsNullOrWhiteSpaceToOpacityConverter"/>
        <vh:IndexToGroupNameConverter x:Key="IndexToGroupNameConverter"/>
        <vh:AlarmTypeToColourConverter x:Key="AlarmTypeToColourConverter"/>

        <ControlTemplate x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
            <Track x:Name="PART_Track" 
                   IsDirectionReversed="True"
                   ViewportSize="NaN"
                   Margin="0,14,0,0" 
                   Width="20">
                <Track.Thumb>
                    <Thumb Style="{StaticResource ScrollBarThumb}" MinHeight="30" Margin="0,1,9,3.5">
                        <Thumb.BorderBrush>
                            <SolidColorBrush Color="{StaticResource ColourScrollBarThumb}"/>
                        </Thumb.BorderBrush>
                        <Thumb.Background>
                            <SolidColorBrush Color="{StaticResource ColourScrollBarThumb}"/>
                        </Thumb.Background>
                    </Thumb>
                </Track.Thumb>
            </Track>
        </ControlTemplate>
        <Style TargetType="{x:Type ScrollBar}">
            <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false"/>
            <Setter Property="Stylus.IsFlicksEnabled" Value="false"/>
            <Setter Property="Width" Value="14"/>
            <Setter Property="Margin" Value="-14,-14,-8,-2" />
            <Setter Property="MinWidth" Value="{Binding Width, RelativeSource={RelativeSource Self}}"/>
            <Setter Property="MinHeight" Value="{Binding Height, RelativeSource={RelativeSource Self}}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid x:Name="Bg" SnapsToDevicePixels="true">
                            <Border Padding="0 4">
                                <Track x:Name="PART_Track"
                                       IsDirectionReversed="true"
                                       IsEnabled="{TemplateBinding IsMouseOver}"
                                       Width="10"
                                       HorizontalAlignment="Center" >
                                    <Track.Thumb>
                                        <Thumb Style="{StaticResource ScrollBarThumb}"/>
                                    </Track.Thumb>
                                </Track>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="Orientation" Value="Horizontal">
                    <Setter Property="Width" Value="Auto" />
                    <Setter Property="Height" Value="14" />
                    <Setter Property="Template" Value="{StaticResource HorizontalScrollBar}" />
                </Trigger>
                <Trigger Property="Orientation" Value="Vertical">
                    <Setter Property="Width" Value="18" />
                    <Setter Property="Height" Value="Auto" />
                    <Setter Property="Template" Value="{StaticResource VerticalScrollBar}" />
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="ButtonFocusVisual">
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Rectangle Margin="5" SnapsToDevicePixels="true" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--<Style x:Key="AlarmButtonStyle" TargetType="{x:Type ToggleButton}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource ButtonFocusVisual}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Grid x:Name="grid">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Cursor" Value="Hand"/>
                            </Trigger>
                            <Trigger Property="IsEnabled" Value="False">
                                <Setter Property="Opacity" Value="0.4"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>-->

        <Style x:Key="GridIconStyle" TargetType="{x:Type ToggleButton}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource ButtonFocusVisual}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Grid x:Name="grid">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Cursor" Value="Hand"/>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="False">
                                <Setter Property="Background" TargetName="grid" Value="Transparent"/>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter Property="Background" TargetName="grid" Value="{StaticResource SelectedDeviceBackground}"/>
                                <Setter Property="Opacity" Value="1"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </Page.Resources>


    <Grid Margin="10,5,0,0">

        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Text="{Binding PageHeader}" Style="{StaticResource PageTitle}"/>


            <!--#region properties-->
            <Border Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" 
                    Style="{StaticResource InputPanelBorder}"
                    Margin="0,3,0,8" Padding="10" VerticalAlignment="Top">

                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>

                    <StackPanel Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,0,0,0">
                        <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding PanelSounder1GroupDesc}" HorizontalAlignment="Right" Margin="0,0,0,0"/>
                        <ctc:NumberSpinner Grid.Row="0" Grid.Column="1"
                                           Value="{Binding PanelSounder1Group}" 
                                           Minimum="{Binding MinSounderGroup}"
                                           Maximum="{Binding MaxSounderGroup}"
                                           PreviewMouseDown="ctrl_PreviewEvent"
                                           PreviewMouseWheel="ctrl_PreviewMouseWheel" 
                                           Margin="5,4,0,0"/>
                    </StackPanel>

                    <StackPanel Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Orientation="Horizontal" HorizontalAlignment="Left" Margin="0,0,0,0">
                        <TextBlock Grid.Row="1" Grid.Column="0" Text="{Binding PanelSounder2GroupDesc}" HorizontalAlignment="Right" Margin="0,0,0,0"/>
                        <ctc:NumberSpinner Grid.Row="1" Grid.Column="1" 
                                           Value="{Binding PanelSounder2Group}"
                                           Minimum="{Binding MinSounderGroup}"
                                           Maximum="{Binding MaxSounderGroup}"
                                           PreviewMouseDown="ctrl_PreviewEvent"
                                           PreviewMouseWheel="ctrl_PreviewMouseWheel" 
                                           Margin="5,3,0,0"/>
                    </StackPanel>


                    <TextBlock Grid.Row="0" Grid.Column="2" Text="{Binding Path=Evac_Tone, Source={StaticResource Resources}}" HorizontalAlignment="Right" Margin="25,0,0,0"/>
                    <ComboBox Grid.Row="0" Grid.Column="3" 
                              ItemsSource="{Binding EvacTones}"
                              SelectedIndex="{Binding EvacTone}"
                              PreviewMouseDown="combo_PreviewMouseDown"
                              PreviewKeyDown="ctrl_PreviewEvent"
                              MinWidth="80" HorizontalAlignment="Left" MaxDropDownHeight="500"
                              Margin="5,3,0,0"/>

                    <TextBlock Grid.Row="1" Grid.Column="2" Text="{Binding Path=Alert_Tone, Source={StaticResource Resources}}" 
                               Visibility="{Binding CurrentProtocolIsCast, Converter={StaticResource BoolToVisibilityConverter}}" 
                               HorizontalAlignment="Right" Margin="25,0,0,0"/>
                    <ComboBox Grid.Row="1" Grid.Column="3" 
                              ItemsSource="{Binding AlertTones}"
                              SelectedIndex="{Binding AlertTone, Mode=TwoWay}"
                              PreviewMouseDown="combo_PreviewMouseDown"
                              PreviewKeyDown="ctrl_PreviewKeyDown"
                              MinWidth="80" HorizontalAlignment="Left" MaxDropDownHeight="500"
                              Visibility="{Binding CurrentProtocolIsCast, Converter={StaticResource BoolToVisibilityConverter}}"
                              Margin="5,0,0,0"/>

                    
                    <CheckBox Grid.Row="0" Grid.Column="4" Content="{Binding Path=New_Fire_Causes_Resound, Source={StaticResource Resources}}"
                              IsChecked="{Binding NewFireCausesResound}"
                              PreviewMouseDown="ctrl_PreviewEvent"
                              PreviewKeyDown="ctrl_PreviewEvent"
                              FontSize="12"
                              HorizontalAlignment="Left"
                              Margin="30,4,0,-2"/>

                    <WrapPanel Grid.Row="1" Grid.Column="4" Orientation="Horizontal">
                        <TextBlock Text="{Binding Path=Phased_Delay, Source={StaticResource Resources}}" Margin="30,0,0,0"/>
                        <ctc:TimePicker x:Name="tpPhasedDelay"
                                        Value="{Binding PhasedDelay}" Format="mmss" MaxValue="10:00" SecondsIncrement="30" MinutesIncrement="1" 
                                        PreviewMouseDown="ctrl_PreviewEvent"
                                        PreviewMouseWheel="ctrl_PreviewMouseWheel"
                                        Margin="3,3,3,0"/>
                        <TextBlock Text="{Binding Path=Minute_Short, Source={StaticResource Resources}}"/>
                    </WrapPanel>
                </Grid>
            </Border>
            <!--#endregion-->


            <!--#region button controls-->
            <StackPanel Grid.Row="2" Grid.Column="0"
                        Orientation="Horizontal" Margin="3,5,10,0">

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding Path=Set_Selected_Items, Source={StaticResource Resources}}" FontSize="11" Margin="0,0,0,2"/>

                    <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">

                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="AlarmOffButton_Click"
                                Height="48"
                                Margin="0,0,0,0">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="26" Height="26" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmOff, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=AlarmType_Off, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>

                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="AlarmAlertButton_Click"
                                Height="48"
                                Margin="5,0,0,0">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="26" Height="26" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmAlert, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=AlarmType_Alert, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>

                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="AlarmEvacButton_Click"
                                Height="48"
                                Margin="5,0,0,0">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="26" Height="26" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmEvac, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=AlarmType_Evacuate, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>

                    </StackPanel>


                    <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Horizontal" Margin="50,0,0,0">
                        <Button Margin="0,0,0,0" VerticalAlignment="Center" Click="SetAllOff_Click" Height="48">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="22" Height="22" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmOff, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=Set_All_To_Off, Source={StaticResource Resources}}" FontSize="10" HorizontalAlignment="Center" TextWrapping="Wrap" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>
                        <Button Margin="5,0,0,0" VerticalAlignment="Center" Click="SetAllAlert_Click" Height="48">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="22" Height="22" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmAlert, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=Set_All_To_Alert, Source={StaticResource Resources}}" FontSize="10" HorizontalAlignment="Center" TextWrapping="Wrap" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>
                        <Button Margin="5,0,0,0" VerticalAlignment="Center" Click="SetAllEvac_Click" Height="48">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="22" Height="22" Margin="0,0,9,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding AlarmEvac, Converter={StaticResource AlarmTypeToColourConverter}}" Style="{StaticResource AlarmIcon}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=Set_All_To_Evac, Source={StaticResource Resources}}" FontSize="10" HorizontalAlignment="Center" TextWrapping="Wrap" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>
                    </StackPanel>
                </Grid>

            </StackPanel>
            <!--#endregion-->


            <!--#region main grid-->
            <WrapPanel Grid.Row="3" Grid.Column="0">
                <Border Background="{StaticResource Brush02}" BorderBrush="{StaticResource Brush02}" BorderThickness="1" CornerRadius="10,10,0,0" Margin="0,5,5,5">
                    <Grid Margin="1,5,1,1" Background="{StaticResource Brush10}">

                        <Border Margin="-2,-5,-2,1" Background="{StaticResource Brush02}" BorderBrush="{StaticResource Brush02}" BorderThickness="1" CornerRadius="8,8,0,0" Height="40" VerticalAlignment="Top"/>

                        <Grid Margin="0,0,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="4"/>
                                <ColumnDefinition Width="{Binding ZoneNameWidth}"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="1"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="1"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="24"/>
                                <ColumnDefinition Width="1"/>
                                <ColumnDefinition Width="32"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="36"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <!--#region column headings-->
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="22" Background="{StaticResource Brush02}" Margin="-1,-1,0,1"/>

                            <TextBlock Grid.Row="0" Grid.Column="1" Text="{Binding Path=Zone, Source={StaticResource Resources}}" Foreground="{StaticResource Brush10}" VerticalAlignment="Bottom" Margin="5,0,0,2"/>

                            <Grid Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="20" Background="{StaticResource Brush02}" Margin="-3,0,0,1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>

                                <TextBlock Grid.Row="0" Grid.Column="0" Text="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="1" Text="2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="2" Text="3" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="3" Text="4" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="4" Text="5" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="6" Text="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="7" Text="7" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="8" Text="8" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="9" Text="9" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="10" Text="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="12" Text="11" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="13" Text="12" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="14" Text="13" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="15" Text="14" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="16" Text="15" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="0" Grid.Column="18" Text="16" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="4,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                            </Grid>

                            <TextBlock Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="20" Text="{Binding Path=Triggers_Sounder_Groups, Source={StaticResource Resources}}" FontSize="14" FontWeight="DemiBold" Foreground="{StaticResource Brush07}" HorizontalAlignment="Center" Margin="0,-5,30,20"/>
                            <!--#endregion-->

                            <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="21" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="0">
                                <Grid x:Name="grdGroupConfig" SizeChanged="Grid_SizeChanged" Margin="0,0,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="1"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="1"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="24"/>
                                        <ColumnDefinition Width="1"/>
                                        <ColumnDefinition Width="28"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                        <RowDefinition Height="24"/>
                                    </Grid.RowDefinitions>

                                    <!--#region zone/panel numbers-->
                                    <Grid Grid.Row="1" Grid.Column="0" Background="{StaticResource Brush09}" Margin="0"><TextBlock Text="1" HorizontalAlignment="Right" Padding="5,0,5,0"/></Grid>
