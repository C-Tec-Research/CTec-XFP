<Page x:Class="Xfp.UI.Views.PanelTools.SetsConfig"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Xfp.UI.Views.PanelTools"
      xmlns:ctc="clr-namespace:CTecControls.UI;assembly=CTecControls" 
      xmlns:ctcb="clr-namespace:CTecControls.UI.Behaviours;assembly=CTecControls" 
      xmlns:ctcvh="clr-namespace:CTecControls.UI.ViewHelpers;assembly=CTecControls"
      xmlns:viewmodels="clr-namespace:Xfp.ViewModels.PanelTools" 
      xmlns:vh="clr-namespace:Xfp.UI.ViewHelpers"
      xmlns:val="clr-namespace:Xfp.UI.Validation"
      d:DataContext="{d:DesignInstance Type=viewmodels:SetConfigViewModel}"
      mc:Ignorable="d" 
      Title="SetConfig">

    <Page.Resources>
        <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <ctcvh:BoolToInverseConverter x:Key="BoolToInverseConverter"/>
        <ctcvh:InverseBoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter"/>
        <ctcvh:BoolToOpacity40pcConverter x:Key="BoolToOpacity40pcConverter"/>
        <ctcvh:InverseBoolToOpacity40pcConverter x:Key="InverseBoolToOpacity40pcConverter"/>
        <ctcvh:InverseBoolToErrorColourConverter x:Key="InverseBoolToErrorColourConverter"/>
        <ctcvh:IsNullOrWhiteSpaceToBoolConverter x:Key="IsNullOrWhiteSpaceToBoolConverter"/>
        <ctcvh:IsNullOrWhiteSpaceToOpacityConverter x:Key="IsNullOrWhiteSpaceToOpacityConverter"/>
        <vh:SetTriggerTypeToColourConverter x:Key="SetTriggerTypeToColourConverter"/>
        <vh:SetTriggerTypeToSvgStringConverter x:Key="SetTriggerTypeToSvgStringConverter"/>

        <ControlTemplate x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
            <Track x:Name="PART_Track" 
                   IsDirectionReversed="True"
                   ViewportSize="NaN"
                   Margin="0,14,0,0" 
                   Width="20">
                <Track.Thumb>
                    <Thumb Style="{StaticResource ScrollBarThumb}" MinHeight="30" Margin="0,1,9,3.5">
                        <Thumb.BorderBrush>
                            <SolidColorBrush Color="{StaticResource ColourScrollBarThumb}"/>
                        </Thumb.BorderBrush>
                        <Thumb.Background>
                            <SolidColorBrush Color="{StaticResource ColourScrollBarThumb}"/>
                        </Thumb.Background>
                    </Thumb>
                </Track.Thumb>
            </Track>
        </ControlTemplate>
        <Style TargetType="{x:Type ScrollBar}">
            <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false"/>
            <Setter Property="Stylus.IsFlicksEnabled" Value="false"/>
            <Setter Property="Width" Value="14"/>
            <Setter Property="Margin" Value="-14,-14,-8,-2" />
            <Setter Property="MinWidth" Value="{Binding Width, RelativeSource={RelativeSource Self}}"/>
            <Setter Property="MinHeight" Value="{Binding Height, RelativeSource={RelativeSource Self}}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid x:Name="Bg" SnapsToDevicePixels="true">
                            <Border Padding="0 4">
                                <Track x:Name="PART_Track"
                                       IsDirectionReversed="true"
                                       IsEnabled="{TemplateBinding IsMouseOver}"
                                       Width="10"
                                       HorizontalAlignment="Center" >
                                    <Track.Thumb>
                                        <Thumb Style="{StaticResource ScrollBarThumb}"/>
                                    </Track.Thumb>
                                </Track>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="Orientation" Value="Horizontal">
                    <Setter Property="Width" Value="Auto" />
                    <Setter Property="Height" Value="14" />
                    <Setter Property="Template" Value="{StaticResource HorizontalScrollBar}" />
                </Trigger>
                <Trigger Property="Orientation" Value="Vertical">
                    <Setter Property="Width" Value="18" />
                    <Setter Property="Height" Value="Auto" />
                    <Setter Property="Template" Value="{StaticResource VerticalScrollBar}" />
                </Trigger>
            </Style.Triggers>
        </Style>

        <Style x:Key="GridIconStyle" TargetType="{x:Type ToggleButton}">
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="0"/>
            <Setter Property="Width" Value="20"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ToggleButton}">
                        <Grid x:Name="grid">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Cursor" Value="Hand"/>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="False">
                                <Setter Property="Background" TargetName="grid" Value="Transparent"/>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="True">
                                <Setter Property="Background" TargetName="grid" Value="{StaticResource SelectedDeviceBackground}"/>
                                <Setter Property="Opacity" Value="1"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

    </Page.Resources>

    <Grid Margin="10,5,0,0">

        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Text="{Binding PageHeader}" Style="{StaticResource PageTitle}"/>


            <!--#region button controls-->
            <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"
                        Orientation="Horizontal" Margin="3,0,10,0">

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <TextBlock Grid.Row="0" Grid.Column="0" Text="{Binding Path=Set_Selected_Items, Source={StaticResource Resources}}" FontSize="11" Margin="0,0,0,2"/>

                    <!--#region trigger types-->
                    <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal">

                        <!--#region set not triggered-->
                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="NotTriggeredButton_Click"
                                PreviewMouseDown="ctrl_PreviewMouseDown"
                                ToolTip="{Binding Path=ToolTip_Not_Triggered, Source={StaticResource Resources}}"
                                Height="50" 
                                MinWidth="70">
                            <StackPanel Orientation="Vertical">
                                <Viewbox Width="26" Height="26" Margin="0,0,5,0" HorizontalAlignment="Center">
                                    <Path Fill="{Binding TriggerNotTriggered, Converter={StaticResource SetTriggerTypeToColourConverter}}" Data="{Binding TriggerNotTriggered, Converter={StaticResource SetTriggerTypeToSvgStringConverter}}"/>
                                </Viewbox>
                                <TextBlock Text="{Binding Path=TriggerType_Not_Triggered, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                            </StackPanel>
                        </Button>
                        <!--#endregion-->

                        <!--#region set pulsed-->
                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="PulsedButton_Click"
                                PreviewMouseDown="ctrl_PreviewMouseDown"
                                ToolTip="{Binding Path=ToolTip_Pulsed, Source={StaticResource Resources}}"
                                Height="50" 
                                MinWidth="70"
                                Margin="5,0,0,0">
                            <Grid>
                                <StackPanel Orientation="Vertical">
                                    <Viewbox Width="26" Height="26" Margin="0,0,5,0" HorizontalAlignment="Center">
                                        <Path Fill="{Binding TriggerPulsed, Converter={StaticResource SetTriggerTypeToColourConverter}}" Data="{Binding TriggerPulsed, Converter={StaticResource SetTriggerTypeToSvgStringConverter}}"/>
                                    </Viewbox>
                                    <TextBlock Text="{Binding Path=TriggerType_Pulsed, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                                </StackPanel>
                            </Grid>
                        </Button>
                        <!--#endregion-->

                        <!--#region set continuous-->
                        <Button BorderThickness="0"
                                Focusable="True"
                                Click="ContinuousButton_Click"
                                PreviewMouseDown="ctrl_PreviewMouseDown"
                                ToolTip="{Binding Path=ToolTip_Continuous, Source={StaticResource Resources}}"
                                Height="50" 
                                MinWidth="70"
                                Margin="5,0,0,0">
                            <Grid>
                                <StackPanel Orientation="Vertical">
                                    <Viewbox Width="26" Height="26" Margin="0,0,5,0" HorizontalAlignment="Center">
                                        <Path Fill="{Binding TriggerContinuous, Converter={StaticResource SetTriggerTypeToColourConverter}}" Data="{Binding TriggerContinuous, Converter={StaticResource SetTriggerTypeToSvgStringConverter}}"/>

                                        <!--<Path Fill="Green" Data="M11 6.04029C11 4.78253 9.5451 4.08327 8.56296 4.86899L4.64922 7.99998H3C1.34315 7.99998 0 9.34312 0 11V13C0 14.6568 1.34315 16 3 16H4.64922L8.56296 19.131C9.5451 19.9167 11 19.2174 11 17.9597V6.04029ZM5.89861 9.56172L9 7.0806V16.9194L5.89861 14.4382C5.54398 14.1545 5.10336 14 4.64922 14H3C2.44772 14 2 13.5523 2 13V11C2 10.4477 2.44772 9.99998 3 9.99998H4.64922C5.10336 9.99998 5.54398 9.84542 5.89861 9.56172ZM17.0711 5.12124C16.6805 4.73072 16.0474 4.73072 15.6568 5.12124C15.2663 5.51177 15.2663 6.14493 15.6568 6.53545C18.781 9.65965 18.781 14.725 15.6568 17.8492C15.2663 18.2397 15.2663 18.8729 15.6568 19.2634C16.0474 19.6539 16.6805 19.6539 17.0711 19.2634C20.9763 15.3581 20.9763 9.02648 17.0711 5.12124ZM14.2426 7.94967C13.8521 7.55914 13.2189 7.55914 12.8284 7.94967C12.4379 8.34019 12.4379 8.97336 12.8284 9.36388C14.3905 10.926 14.3905 13.4586 12.8284 15.0207C12.4379 15.4113 12.4379 16.0444 12.8284 16.4349C13.2189 16.8255 13.8521 16.8255 14.2426 16.4349C16.5858 14.0918 16.5858 10.2928 14.2426 7.94967Z"/>-->

                                    </Viewbox>
                                    <TextBlock Text="{Binding Path=TriggerType_Continuous, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                                </StackPanel>
                            </Grid>
                        </Button>
                        <!--#endregion-->

                        <!--#region set delayed-->
                        <Border BorderThickness="1" BorderBrush="{StaticResource Brush07}" CornerRadius="7" Padding="2" Margin="5,0,0,0">
                            <StackPanel Orientation="Horizontal">
                                <Button BorderThickness="0"
                                        Focusable="True"
                                        Click="DelayedButton_Click"
                                        PreviewMouseDown="ctrl_PreviewMouseDown"
                                        Height="50" 
                                        MinWidth="70"
                                        Margin="0,0,0,0">
                                    <Grid>
                                        <StackPanel Orientation="Horizontal">
                                            <StackPanel Orientation="Vertical">
                                                <Viewbox Width="26" Height="26" Margin="0,0,5,0" HorizontalAlignment="Center">
                                                    <Path Fill="{Binding TriggerDelayed, Converter={StaticResource SetTriggerTypeToColourConverter}}" Data="{Binding TriggerDelayed, Converter={StaticResource SetTriggerTypeToSvgStringConverter}}"/>
                                                </Viewbox>
                                                <TextBlock Text="{Binding Path=TriggerType_Delayed, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </Grid>
                                </Button>
                                <StackPanel Grid.Row="1" Grid.Column="1" Orientation="Vertical" VerticalAlignment="Center" Margin="5,0,0,0">
                                    <TextBlock Text="{Binding Path=Delay_Time, Source={StaticResource Resources}}" FontSize="11" Margin="3,0,0,0"/>
                                    <Border Grid.Row="1" Grid.Column="1" BorderBrush="{Binding DelayTimerIsValid, Converter={StaticResource InverseBoolToErrorColourConverter}}" BorderThickness="2" Margin="-1,0,0,0">
                                        <StackPanel Orientation="Horizontal">
                                            <ctc:TimePicker x:Name="tpDelay"
                                                            Value="{Binding DelayTimer}" Format="mmss" MaxValue="10:00" SecondsIncrement="30"
                                                            PreviewMouseDown="ctrl_PreviewMouseDown" 
                                                            PreviewKeyDown="ctrl_PreviewKeyDown"
                                                            PreviewMouseWheel="ctrl_PreviewMouseWheel"
                                                            Margin="1,1,0,0"/>
                                            <TextBlock Text="{Binding Path=Minute_Short, Source={StaticResource Resources}}"/>
                                        </StackPanel>
                                    </Border>
                                </StackPanel>
                            </StackPanel>
                        </Border>
                        <!--#endregion-->
                        
                    </StackPanel>
                    <!--#endregion-->
                        
                    <!--#region clear all-->
                    <Button Grid.Row="1" Grid.Column="2" BorderThickness="0"
                            Focusable="True"
                            Click="ClearAll_Click"
                            PreviewMouseDown="ctrl_PreviewMouseDown"
                            ToolTip="{Binding Path=ToolTip_Clear_All, Source={StaticResource Resources}}"
                            Height="50" 
                            MinWidth="80"
                            Margin="30,0,0,0">
                        <StackPanel Orientation="Vertical">
                            <Viewbox Width="26" Height="26" Margin="0,0,5,0" HorizontalAlignment="Center">
                                <Path Fill="{Binding TriggerNotTriggered, Converter={StaticResource SetTriggerTypeToColourConverter}}" Data="{Binding TriggerNotTriggered, Converter={StaticResource SetTriggerTypeToSvgStringConverter}}"/>
                            </Viewbox>
                            <TextBlock Text="{Binding Path=Clear_All, Source={StaticResource Resources}}" FontSize="11" HorizontalAlignment="Center" Margin="0,0,5,-2"/>
                        </StackPanel>
                    </Button>
                    <!--#endregion-->

                </Grid>

            </StackPanel>
            <!--#endregion-->


            <!--#region main grid-->
            <WrapPanel Grid.Row="3" Grid.Column="0" Margin="0,10,0,0">
                <Border Background="{StaticResource Brush02}" BorderBrush="{StaticResource Brush02}" BorderThickness="1" CornerRadius="8,8,0,0" Margin="0,0,0,5">
                    <Grid Margin="1,5,1,1" Background="{StaticResource Brush10}">

                        <Border Margin="-2,-1,-2,0" Background="{StaticResource Brush02}" BorderBrush="{StaticResource Brush02}" BorderThickness="1" CornerRadius="8,8,0,0" VerticalAlignment="Top"/>

                        <Grid Margin="0,0,0,0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="*"/>
                            </Grid.RowDefinitions>

                            <!--#region column headings-->
                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="23" Background="{StaticResource Brush02}" Margin="0,0,0,1"/>

                            <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="25" x:Name="grdHeaders" SizeChanged="grdHeaders_SizeChanged" Background="{StaticResource Brush02}" Margin="-1,0,0,1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="20"/>
                                    <RowDefinition Height="22"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="{Binding HeaderLeftWidth}"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="1"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="15"/>
                                    <ColumnDefinition Width="24"/>
                                    <ColumnDefinition Width="25"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="19" x:Name="headerRow1" Orientation="Vertical">
                                    <TextBlock Text="{Binding Path=Output_Set_Triggered, Source={StaticResource Resources}}" 
                                               Foreground="{StaticResource Brush10}" 
                                               HorizontalAlignment="Center" Margin="0,0,0,0"/>
                                    <Border BorderThickness="1,1,1,0" BorderBrush="{StaticResource Brush04}" CornerRadius="5,5,0,0" Height="5" Margin="3,5,3,-5"/>
                                </StackPanel>
                                <TextBlock Grid.Row="0" Grid.Column="17" Grid.ColumnSpan="15" 
                                           Text="{Binding Path=Panel_Relay_Triggered, Source={StaticResource Resources}}" 
                                           Foreground="{StaticResource BrushPanelRelayHeader}" 
                                           HorizontalAlignment="Right" VerticalAlignment="Top" 
                                           TextWrapping="Wrap" TextAlignment="Right" 
                                           Width="110" Margin="0,0,5,0"/>

                                <!--#region column headings-->
                                <TextBlock Grid.Row="1" Grid.Column="2" Text="1" x:Name="headerRow2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="3" Text="2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="4" Text="3" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="5" Text="4" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="6" Text="5" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="8" Text="6" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="9" Text="7" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="10" Text="8" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="11" Text="9" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="12" Text="10" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="14" Text="11" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="15" Text="12" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="16" Text="13" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="17" Text="14" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="18" Text="15" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="20" Text="16" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="-1,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource Brush10}"/>
                                <TextBlock Grid.Row="1" Grid.Column="22" Text="1" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="0,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource BrushPanelRelayHeader}"/>
                                <TextBlock Grid.Row="1" Grid.Column="23" Text="2" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="3,0,0,0" FontSize="12" FontWeight="Bold" Foreground="{StaticResource BrushPanelRelayHeader}"/>
                                <!--#endregion-->
                                
                                <!--checkbox background-->
                                <Grid Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="24" Background="{StaticResource Brush07}"/>

                                <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Path=Zone, Source={StaticResource Resources}}" VerticalAlignment="Bottom" Margin="10,0,0,1"/>
                                
                                <!--<TextBlock Grid.Row="2" Grid.Column="1" x:Name="txtSilenceableHeader" Text="Silenceable?" HorizontalAlignment="Right" Foreground="{StaticResource Brush03}" Margin="4,0,3,0"/>-->

                                <!--<StackPanel Grid.Row="2" Grid.Column="1" Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="4,0,0,2">
                                    <TextBlock Text="{Binding Path=Is_Silenceable, Source={StaticResource Resources}}" Margin="0"/>
                                    <TextBlock Text="→" FontSize="20" Margin="0,0,0,1"/>
                                </StackPanel>-->

                                <!--#region silenceable checkboxes-->
                                <CheckBox Grid.Row="2" Grid.Column="2" IsChecked="{Binding OutputSetIsSilenceable[0]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="3" IsChecked="{Binding OutputSetIsSilenceable[1]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="4" IsChecked="{Binding OutputSetIsSilenceable[2]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="5" IsChecked="{Binding OutputSetIsSilenceable[3]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="6" IsChecked="{Binding OutputSetIsSilenceable[4]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>

                                <CheckBox Grid.Row="2" Grid.Column="8" IsChecked="{Binding OutputSetIsSilenceable[5]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="9" IsChecked="{Binding OutputSetIsSilenceable[6]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="10" IsChecked="{Binding OutputSetIsSilenceable[7]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="11" IsChecked="{Binding OutputSetIsSilenceable[8]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="12" IsChecked="{Binding OutputSetIsSilenceable[9]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>

                                <CheckBox Grid.Row="2" Grid.Column="14" IsChecked="{Binding OutputSetIsSilenceable[10]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="15" IsChecked="{Binding OutputSetIsSilenceable[11]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="16" IsChecked="{Binding OutputSetIsSilenceable[12]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="17" IsChecked="{Binding OutputSetIsSilenceable[13]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="18" IsChecked="{Binding OutputSetIsSilenceable[14]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>

                                <CheckBox Grid.Row="2" Grid.Column="20" IsChecked="{Binding OutputSetIsSilenceable[15]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>

                                <CheckBox Grid.Row="2" Grid.Column="22" IsChecked="{Binding PanelRelayIsSilenceable[0]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="4,3,0,0"/>
                                <CheckBox Grid.Row="2" Grid.Column="23" IsChecked="{Binding PanelRelayIsSilenceable[1]}" PreviewMouseDown="ctrl_PreviewMouseDown" Margin="6,3,0,0"/>
                                <!--#endregion-->
                                
                            </Grid>
                            <!--#endregion-->

                            <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="22" HorizontalScrollBarVisibility="Auto" Margin="1,0,0,0">

                                <Grid x:Name="grdSetsConfig">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="20"/>
                                        <RowDefinition Height="22"/>
